        xmlns:local="clr-namespace:GidraSIM.GUI"
        mc:Ignorable="d"
        Title="Редактирование параметров" Height="450" Width="340.644">
    <Grid x:Name="mainGrid">
        <Button x:Name="save" Content="Сохранить" HorizontalAlignment="Left" Margin="130,380,0,0" VerticalAlignment="Top" Width="75" Click="Save_Click"/>
